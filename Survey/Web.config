<?xml version="1.0"?>
<configuration>

  <location path="~/Login.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>

  <system.web>
    <authorization>
        <allow roles="SurveyAdmin" />
        <allow roles="Surveyor" />   
        <deny users="*" />
    </authorization>
  </system.web>

</configuration>
